{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}/Libraries/ppmac_tcp/ppmac_tcp.c",
                "${workspaceFolder}/Libraries/ppmac_ushm_buffer/ppmac_ushm_buffer.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I",
                "${workspaceFolder}/Libraries/ppmac_tcp",
                "-I",
                "${workspaceFolder}/Libraries/ppmac_ushm_buffer"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}